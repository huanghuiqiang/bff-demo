#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/swig-templates@2.0.3/node_modules/swig-templates/bin/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/swig-templates@2.0.3/node_modules/swig-templates/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/swig-templates@2.0.3/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules:/Users/hugh/Desktop/node_modules:/Users/hugh/node_modules:/Users/node_modules:/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/koa-swig@2.2.1/node_modules/koa-swig/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/koa-swig@2.2.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/swig-templates@2.0.3/node_modules/swig-templates/bin/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/swig-templates@2.0.3/node_modules/swig-templates/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/swig-templates@2.0.3/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules:/Users/hugh/Desktop/node_modules:/Users/hugh/node_modules:/Users/node_modules:/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/koa-swig@2.2.1/node_modules/koa-swig/node_modules:/Users/hugh/Desktop/mvc-demo/node_modules/.pnpm/koa-swig@2.2.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../swig-templates@2.0.3/node_modules/swig-templates/bin/swig.js" "$@"
else
  exec node  "$basedir/../../../../../swig-templates@2.0.3/node_modules/swig-templates/bin/swig.js" "$@"
fi
